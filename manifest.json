{
  "manifest_version": 2,

  "name": "ttLibrary",
  "description": "TT music library",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://turntable.fm/*"],
      "css": ["/styles/css/styles.css"],
      "js": [
        "/js/lib/jquery.js",
        "/js/extension-init.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/js/lib/*",
    "/templates/*",
    "/js/*"
  ],
  "permissions": [
    "tabs", "http://turntable.fm/*"
  ]
}